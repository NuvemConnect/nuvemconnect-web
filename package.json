{
  "name": "nuvemconnect-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint --fix",
    "format": "prettier --write \"src/**/*.html\" \"src/**/*.ts\"",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "commitlint": "commitlint --from=HEAD~1",
    "serve:ssr:nuvemconnect-web": "node dist/nuvemconnect-web/server/server.mjs",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "tsc --noEmit",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/platform-server": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/ssr": "^18.1.3",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0",
    "nuvemconnect-web": "file:",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "yarn": "^1.22.22",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.3",
    "@angular/cli": "^18.1.3",
    "@angular/compiler-cli": "^18.1.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "angular-eslint": "18.2.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.7",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.0.0"
  }
}
